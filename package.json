  },
  "dependencies": {
    "glob": "^11.0.0",
    "knitwork": "1.1.0",
    "pathe": "1.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.16.10",
    "@vitest/coverage-v8": "^2.1.3",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0",
    "vite": "^5.4.11",
    "vitest": "^2.1.3"
  },
  "packageManager": "pnpm@9.12.2",
  "peerDependencies": {
    "vite": "^5.4.11"
  }
}
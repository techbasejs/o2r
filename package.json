{
  "name": "@techbasejs/o2r",
  "version": "1.0.2",
  "description": "Convert object to vue or react router",
  "repository": "techbasejs/o2r",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
